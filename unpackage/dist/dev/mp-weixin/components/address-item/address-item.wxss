/* 水平间距 */
.animation {
  -webkit-animation: jelly 0.6s ease;
          animation: jelly 0.6s ease;
}
.address-item {
  display: flex;
  justify-content: space-between;
  padding: 15rpx;
  border-bottom: 1rpx solid #efefef;
  align-items: center;
}
.address-item .address-item-left {
  flex: 1;
  display: flex;
  align-items: center;
}
.address-item .address-item-left .address-item-change {
  flex: 1;
  text-align: center;
}
.address-item .address-item-left .address-info {
  flex: 8;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.address-item .address-item-left .address-info .address-province {
  font-size: 24rpx;
  color: #9f9f9f;
}
.address-item .address-item-left .address-info .address-details {
  margin: 10rpx 0;
  font-weight: 700;
}
.address-item .address-item-left .address-info .address-contact {
  font-size: 24rpx;
}
.address-item .address-item-left .address-info .address-contact text {
  margin-right: 15rpx;
}
.address-item .address-item-left .address-info .address-contact text:nth-child(2) {
  font-size: 22rpx;
  color: #9f9f9f;
}
.address-item .address-icon {
  width: 100rpx;
  text-align: center;
  font-size: 40rpx;
}
@-webkit-keyframes jelly {
from {
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
}
30% {
    -webkit-transform: scale(1.25, 0.75);
            transform: scale(1.25, 0.75);
}
40% {
    -webkit-transform: scale(0.75, 1.25);
            transform: scale(0.75, 1.25);
}
50% {
    -webkit-transform: scale(1.15, 0.85);
            transform: scale(1.15, 0.85);
}
65% {
    -webkit-transform: scale(0.95, 1.05);
            transform: scale(0.95, 1.05);
}
75% {
    -webkit-transform: scale(1.05, 0.95);
            transform: scale(1.05, 0.95);
}
to {
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
}
}
@keyframes jelly {
from {
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
}
30% {
    -webkit-transform: scale(1.25, 0.75);
            transform: scale(1.25, 0.75);
}
40% {
    -webkit-transform: scale(0.75, 1.25);
            transform: scale(0.75, 1.25);
}
50% {
    -webkit-transform: scale(1.15, 0.85);
            transform: scale(1.15, 0.85);
}
65% {
    -webkit-transform: scale(0.95, 1.05);
            transform: scale(0.95, 1.05);
}
75% {
    -webkit-transform: scale(1.05, 0.95);
            transform: scale(1.05, 0.95);
}
to {
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
}
}

